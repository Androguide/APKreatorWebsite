        </activity>
        <activity android:configChanges="orientation|keyboardHidden|screenSize" android:label="@string/presentation" android:name="com.androguide.apkreator.PresentationActivity">
        </activity>
        <activity android:configChanges="orientation|keyboardHidden|screenSize" android:label="@string/app_name" android:name="com.androguide.apkreator.helpers.youtube.PlayVideo">
        </activity>
    </application>

</manifest>
